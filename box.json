{
    "alias": "main.phar",
    "chmod": "0755",
    "directories": ["src","assets","vendor"],
    "files": [
        "LICENSE",
        "assets/banner.txt",
        "assets/sql.ini"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": ["vendor"]
        }
    ],
    "git-version": "package_version",
    "main": "main.php",
    "output": "build/main.phar",
    "stub": true,
    "algorithm" :"SHA512",
    "compression" :"BZ2"
}
